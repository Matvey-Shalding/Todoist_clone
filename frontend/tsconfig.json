{
	"compilerOptions": {
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"noFallthroughCasesInSwitch": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext",
		"types": ["vite/client"]
	},
	"include": [
		"src",
		"frontend/src/components/main/incomingDisplays/Board.tsx",
		"frontend/src/components/main/incomingMenu/DisplayTasksMenu.tsx",
		"frontend/src/components/main/incomingMenu/subMenus/GroupingSubMenu.tsx",
		"frontend/src/components/main/section/SectionTitle.tsx",
		"frontend/src/components/main/Task/bigTask/Task_big.tsx",
		"frontend/src/components/main/Task/smallTask/Task_small.tsx",
		"frontend/src/components/modals/menu/Date.menu.tsx",
		"frontend/src/components/registration/registration",
		"frontend/src/components/registration/LogInRegistaration.tsx",
		"frontend/src/components/registration/SingUpRegistration.tsx",
		"frontend/src/components/ui",
		"frontend/src/context",
		"frontend/src/data",
		"frontend/src/defaultSettings",
		"frontend/src/types",
		"frontend/src/utils",
		"frontend/src/index.tsx",
		"frontend/src/handleSingUp.ts",
		"frontend/src/App.tsx",
		"frontend/src/server.ts",
		"frontend/src/vite-env.d.ts"
, "src/utils/saveTime.ts"	]
}
